{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "lib": ["DOM", "ES2020"],
    "types": ["svelte"],
    "allowJs": false,
    "strict": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@fivem-ui/core": ["src"]
    }
  },
  "include": ["src/**/*"]
  ,
  "exclude": ["src/__tests__/**"]
}
